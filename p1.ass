DATA:
    inicio #A0
    fin #A8
    contador 0
CODE:
    ciclo:
        MOV B,(inicio)
        MOV A,(B)
        MOV B,(fin)
        MOV B,(B)
        INC (inicio)
        CMP A,B
        JLT termino
        MOV B,A
        INC B
        SHR B,B
        SHR A,A
        CMP A,B
        JLT ciclo
        INC (contador)
        JMP ciclo
    termino:
        MOV B,(contador)
        MOV A,(B)